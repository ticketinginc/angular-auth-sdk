{
  "name": "@ticketing/angular-auth-sdk",
  "version": "0.4.1",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n @ticketing/angular-auth-sdk -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "pack": "gulp build && npm pack dist/",
    "publish": "gulp build && npm publish dist",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ticketinginc/angular-auth-sdk"
  },
  "author": {
    "name": "Sven James",
    "email": "sven@ticketingevents.co"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ticketinginc/angular-auth-sdk/issues"
  },
  "devDependencies": {
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@compodoc/compodoc": "^1.0.0-beta.10",
    "@types/jasmine": "2.8.6",
    "@types/node": "~10.0.0",
    "angular-in-memory-web-api": "^0.6.0",
    "codelyzer": "~4.3.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "fs-extra": "^5.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.15.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.3.1",
    "rollup": "^0.58.2",
    "run-sequence": "^2.2.1",
    "rxjs": "^5.5.10",
    "systemjs": "^0.21.3",
    "ts-node": "~6.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.8.3",
    "zone.js": "^0.8.14"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@ticketing/angular-core-sdk": "^0.7.2"
  }
}
