{
  "name": "@ticketing/angular-auth-sdk",
  "version": "0.5.0",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n @ticketing/angular-auth-sdk -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "pack": "gulp build && npm pack dist/",
    "publish": "gulp build && npm publish dist",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ticketinginc/angular-auth-sdk"
  },
  "author": {
    "name": "Sven James",
    "email": "sven@ticketingevents.co"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ticketinginc/angular-auth-sdk/issues"
  },
  "devDependencies": {
    "@angular/common": "^8.2.9",
    "@angular/compiler": "^8.2.9",
    "@angular/compiler-cli": "^8.2.9",
    "@angular/core": "^8.2.9",
    "@angular/platform-browser": "^8.2.9",
    "@angular/platform-browser-dynamic": "^8.2.9",
    "@compodoc/compodoc": "^1.1.10",
    "@types/jasmine": "3.4.2",
    "@types/node": "~12.7.11",
    "angular-in-memory-web-api": "^0.9.0",
    "codelyzer": "~5.1.2",
    "concurrently": "^5.0.0",
    "core-js": "^3.2.1",
    "fs-extra": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-rollup": "^2.16.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lite-server": "^2.5.4",
    "node-sass": "^4.12.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-watch": "^0.6.3",
    "protractor": "~5.4.2",
    "rollup": "^1.23.1",
    "run-sequence": "^2.2.1",
    "rxjs": "^6.5.3",
    "systemjs": "^6.1.3",
    "ts-node": "~8.4.1",
    "tsickle": "^0.37.0",
    "tslint": "~5.20.0",
    "typescript": "^3.5.3",
    "zone.js": "^0.10.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@angular/forms": "^8.2.9",
    "@angular/http": "^7.2.15",
    "@ticketing/angular-core-sdk": "^0.7.4",
    "npm-check-updates": "^3.1.24"
  }
}
